{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "blue": {
            "Address": "http://localhost:5000"
          },
          "red": {
            "Address": "http://localhost:5001"
          },
          "yellow": {
            "Address": "http://localhost:5002"
          }
        }
      }
    }
  }
}
